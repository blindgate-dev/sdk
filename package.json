{
  "name": "blindgate",
  "version": "0.0.1",
  "description": "Blindgate SDK monorepo",
  "license": "MIT",
  "packageManager": "bun@1.3.7",
  "workspaces": [
    "packages/*",
    "internals/*"
  ],
  "catalog": {
    "bun-types": "1.3.7",
    "typescript": "^5.9.3",
    "@biomejs/biome": "2.3.14"
  },
  "catalogs": {
    "dev": {
      "typescript": "^5.9.3",
      "bun-types": "1.3.7"
    }
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "typecheck": "turbo run typecheck",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "api:generate": "bun run --cwd internals/api generate",
    "clean": "turbo run clean && rm -rf node_modules bun.lock"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "bun-types": "catalog:",
    "turbo": "^2.8.3",
    "typescript": "catalog:"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "bun"
  ]
}
