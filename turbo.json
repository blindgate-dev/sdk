{
  "$schema": "https://turbo.build/schema.json",
  "concurrency": "15",
  "globalDependencies": [".env"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^build"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "generate": {
      "cache": false
    },
    "clean": {
      "cache": false
    }
  }
}
